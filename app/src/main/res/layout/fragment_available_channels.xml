<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.example.wifi.ui.access_points.AccessPointMainView
        android:id="@+id/availableChannelsAccessPointView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        android:autoLink="web"
        android:gravity="center"
        android:text="@string/wifi_channels_list_url" />

    <include layout="@layout/channel_available_list_details"
        android:id="@+id/availableChannelsList"/>

</LinearLayout>